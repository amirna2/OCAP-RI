<!-- Simple SCDF file that describes an improperly signed file -->
<!-- Simple SCDF XML to be used with RegisteredApiManager.register() -->
<?xml version="1.0"?>
<!DOCTYPE applicationdescription
          PUBLIC "-//OCAP//DTD Font Directory 1.0//EN" 
                 "http://www.opencable.com/ocap/dtd/applicationdescriptionfile-1-0.dtd">

<applicationdescription>

  <!-- We need a directory called "unsigned" that has unsigned files -->
  <dir name="unsigned">
    <file name="*" size="ignored"/>
  </dir>

</applicationdescription>
